create table ALUNOS (
 	cpf varchar(11) primary key,
    nome varchar(255) not null,
    digital varchar(255) not null,
    telefone varchar(30) not null,
    sexo char(1) not null,
    foto varchar(255) not null,
    dt_nascimento date not null,
    cod_plan varchar(255) references plano (cod_plan),
    altura numeric not null,
    peso numeric not null
)

select * from alunos

select al.* from alunos al natural join plano pl where pl.categoria = 'b'

drop table Alunos cascade

delete from Alunos

insert into alunos (cpf,nome,digital,telefone,sexo,foto,dt_nascimento,altura,peso,cod_plan) 
Values ('123a13','leo','asdmalsdnajcnsdh','123834','m','1283123skdmxaskl','2023-08-10',1.8,81.6,1)

create table EMAIL (
	cod_e serial primary key,
	cpf varchar(11) not null,
	email varchar not null,
	foreign key (cpf) references alunos(cpf) ON DELETE CASCADE
)

select * from email

drop table email

delete from email

insert into email (cpf, email) values ('12313', 'asdasd')

create table ENDERECO (
	cod_en serial primary key,
	cpf varchar(11) not null,
	rua varchar  not null,
    bairro varchar (11) not null,
    numero int not null, 
    cidade varchar  not null,
    uf varchar(2) not null,
	foreign key (cpf) references alunos(cpf) ON DELETE CASCADE
	)
	
	select * from ENDERECO
	
	delete from endereco
	
	drop table ENDERECO
	
	insert into endereco (cpf,rua,bairro,numero,cidade,uf) values ('12313', 'adasd','asdas', 0, 'asdas', 'sp')
	
	
CREATE TABLE PLANO (
	cod_plan varchar primary key,
	dt_vencimento date not null,
	preco_ano numeric not null,
	preco_mes numeric not null,
	categoria varchar not null 
)

select * from plano

drop table plano

insert into Plano (cod_plan, dt_vencimento,preco_ano,preco_mes,categoria) values ('1','29-09-2012',1200,34.90,'b')
